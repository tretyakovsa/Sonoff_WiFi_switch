{
 "configs":[
  "/config.setup.json",
  "/lang/lang.{{lang}}.json"
 ],
 "content":[],
 "top-content":[],
 "header-left":[
  {
   "type":"h1",
   "title":"{{space}}."
  },
  {
   "type":"h3",
   "name":"update",
   "title":"{{SSDP}}",
   "style":"color:#999;margin-top:-10px"
  },
  {
   "type":"link",
   "class":"btn btn-default top",
   "action":"#",
   "title":"TOP"
  }
 ],
 "header-right":[
  {
   "type":"link",
   "class":"btn btn-info dropdown-toggle",
   "action":"/page.htm?scenary",
   "title":"{{LangScenary}}"
  },
  {
   "type":"link",
   "class":"btn btn-default dropdown-toggle",
   "action":"/page.htm?timer",
   "title":"<i class=\"clock-img\"></i> <span class=\"hidden-xs\">{{LangTimer}}</span>"
  },
  {
   "type":"dropdown",
   "name":"help-url",
   "class":"btn btn-default",
   "title":{
    "#":"<i class=\"help-img\"></i> <span class=\"hidden-xs\">{{LangHelp}}</span> <span class=\"caret\"></span>",
    "https://github.com/tretyakovsa/Sonoff_WiFi_switch/issues":"<b>Sonoff</b> (Relay)",
    "https://github.com/renat2985/rgb/issues":"<b>RGB</b> (WS2811-12/NeoPixel)",
    "https://github.com/tretyakovsa/jaluzi/issues":"<b>Jalousie</b> (Motor)"
   }
  },
  {
   "type":"dropdown",
   "name":"design",
   "class":"btn btn-warning",
   "title":{
    "#":"<span class=\"hidden-xs\">{{LangDesign}}</span> <span class=\"caret\"></span>",
    "/skins?set=index.htm":"<b>Default</b>",
    "/skins?set=index2.htm":"<b>Inline</b>",
    "/skins?set=index3.htm":"<b>Block</b>",
    "/skins?set=index4.htm":"<b>Life</b>"
   }
  },
  {
   "type":"select",
   "state":"{{lang}}",
   "name":"languages",
   "action":"/lang?set=[[languages]]",
   "response":"/",
   "style":"width:70px",
   "class":"hidden-xs",
   "title":{"ru":"RU","en":"EN","lv":"LV","ua":"UA"}
  }
 ]
}
