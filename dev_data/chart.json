{
 "configs":[
  "/config.live.json",
  "/config.options.json",
  "/lang/lang.{{lang}}.json",
  "/modules.json"
 ],
 "title": [{
   "type":"text",
   "title":"{{LangChart}}"
  }],
 "class":"col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
 "content":[
  {
   "type":"h5",
   "title":"{{SSDP}}",
   "class":"alert-default"
  },

  {
   "type":"loadJson",
   "state":"header.json"
  },

  {
   "type":"chart",
   "title":"{{LangChart}}",
   "state":"charts.json?data={{urlArray1}}&data1={{urlArray2}}&data2={{urlArray3}}",
   "style":"height:400px;margin-left:-20px",
   "options":"fullWidth:true"
  },


  {
   "type":"link",
   "title":"<i class=\"opt-img\"></i> {{LangSetting3}}",
   "action":"/page.htm?setup",
   "class":"btn btn-block btn-default"
  },
  {
   "type":"link",
   "module":"admin",
   "title":"{{LangReset1}}",
   "action":"javascript:if(confirm(renameBlock(jsonResponse,'{{LangReset3}}'))){send_request(this,'/restart?device=ok');toggle('restart-esp');timeout('toggle(\\'restart-esp\\')',20000);}",
   "class":"btn btn-block btn-warning"
  }

 ]
}
